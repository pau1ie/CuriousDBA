{
  "name": "hugo-theme-jane",
  "version": "1.2.0",
  "description": "A simple theme for hugo",
  "main": "",
  "scripts": {
    "build": "webpack",
    "demo": "hugo --contentDir='./exampleSite/content' --config='dev-config.toml' ",
    "dev:assets": "webpack --watch --display='errors-only' ",
    "dev:site": "hugo server --buildDrafts --contentDir='./exampleSite/content' --config='dev-config.toml' ",
    "dev": "concurrently 'npm run dev:assets' 'npm run dev:site' ",
    "clean": "rm -rf ./public",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "author": "xianmin",
  "license": "MIT",
  "devDependencies": {
    "all-contributors-cli": "^4.11.1",
    "autoprefixer": "^8.1.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.10",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.11",
    "iconfont-webpack-plugin": "^1.2.0",
    "postcss-loader": "^2.1.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.2",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "url-loader": "^1.0.1",
    "webpack": "^3.11.0"
  },
  "dependencies": {
    "node-sass": "3.4.1",
    "scuttlebutt": "^5.6.15"
  }
}
